<!-- Our Services Start -->
<div class="our-services section-pad-t30">
    <div class="container">
        <!-- Section Tittle -->
        <div class="row">
            <div class="col-lg-12">
                <div class="section-tittle text-center">
                    <span>FEATURED TOURS Packages</span>
                    <h2>Browse Top Categories </h2>
                </div>
            </div>
        </div>
        
        <div class="row d-flex justify-contnet-center">
            <% let counter = 0; %>
<% let uniqueTitles = new Set(); %>
<% for (let job of jobdata) { %>
  <% let normalizedTitle = job.title.toLowerCase().trim(); %>
  <% if (counter < 4 &&!uniqueTitles.has(normalizedTitle)) { %>
    <div class="col-xl-3 col-lg-3 col-md-4 col-sm-6">
      <div class="single-services text-center mb-30">
        <div class="services-ion">
          <span class="flaticon-tour"></span>
        </div>
        <div class="services-cap">
            <%userid=job._id.toString();%>
            <form action="/job_detail" method="post"><input type="hidden" name="id" value="<%=userid%>"><input class="services-cap" type="submit" value="<%=job.title%>"></form>
        </div>
      </div>
    </div>
    <% uniqueTitles.add(normalizedTitle); %>
    <% counter++; %>
  <% } %>
<% } %>
        </div>
        <!-- More Btn -->
        <!-- Section Button -->
        <div class="row">
            <div class="col-lg-12">
                <div class="browse-btn2 text-center mt-50">
                    <a href="/job_listing" class="border-btn2">Browse All Sectors</a>
                </div>
            </div>
        </div>
    </div>
</div>
